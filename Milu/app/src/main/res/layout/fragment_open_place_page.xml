<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:id="@+id/fragment_container">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/gradient_meetup"
        android:scaleType="centerCrop"/>
    <android.support.v4.widget.NestedScrollView
        android:layout_marginTop="@dimen/dimen_56"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/open_place_ScrollView"
        android:paddingBottom="48dp"
        android:clipToPadding="false"
        android:clipChildren="false">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="170dp"
                android:id="@+id/open_place_event_image"
                android:background="@drawable/sports_fitness"
                android:scaleType="centerCrop"
                android:clickable="true"
                android:onClick="onClickFeedImage"
                android:focusable="true" />

            <ImageButton
                android:id="@+id/livePlaceButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/events_page_live_events_button"
                android:layout_marginTop="134dp"
                android:layout_marginEnd="@dimen/dimen_08"
                android:background="@color/transparent"
                android:layout_alignParentEnd="true"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/open_place_event_image"
                android:id="@+id/open_place_DetailLayout">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="8dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/c_TimeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <charles.com.milu.CustomViews.TitleTextView
                            android:id="@+id/open_place_EventDate"
                            android:layout_width="44dp"
                            android:layout_height="42dp"
                            android:text = "03"
                            android:textColor="@color/Deepsky"
                            android:layout_gravity="center"
                            android:textSize="36sp"
                            android:textAlignment="center"/>

                        <charles.com.milu.CustomViews.TitleTextView
                            android:id="@+id/open_place_EventDateLabel"
                            android:layout_width="11dp"
                            android:layout_height="16dp"
                            android:text = "D"
                            android:layout_marginTop="6dp"
                            android:textColor="@color/color_Oldlavender"
                            android:layout_gravity="top"
                            android:textSize="14sp"
                            android:layout_marginLeft="-2dp"
                            android:textAlignment="center"/>

                        <charles.com.milu.CustomViews.TitleTextView
                            android:id="@+id/open_place_EventHour"
                            android:layout_width="44dp"
                            android:layout_height="42dp"
                            android:text = "22"
                            android:textColor="@color/Deepsky"
                            android:layout_gravity="center"
                            android:textSize="36sp"
                            android:textAlignment="center"/>

                        <charles.com.milu.CustomViews.TitleTextView
                            android:id="@+id/open_place_EventHourLabel"
                            android:layout_width="11dp"
                            android:layout_height="16dp"
                            android:text = "H"
                            android:layout_marginTop="6dp"
                            android:textColor="@color/color_Oldlavender"
                            android:layout_gravity="top"
                            android:textSize="14sp"
                            android:layout_marginLeft="-2dp"
                            android:textAlignment="center"/>

                        <charles.com.milu.CustomViews.TitleTextView
                            android:id="@+id/open_place_EventMin"
                            android:layout_width="44dp"
                            android:layout_height="42dp"
                            android:text = "51"
                            android:textColor="@color/Deepsky"
                            android:layout_gravity="center"
                            android:textSize="36sp"
                            android:textAlignment="center"/>

                        <charles.com.milu.CustomViews.TitleTextView
                            android:id="@+id/open_place_EventMinLabel"
                            android:layout_width="11dp"
                            android:layout_height="16dp"
                            android:text = "M"
                            android:layout_marginTop="6dp"
                            android:textColor="@color/color_Oldlavender"
                            android:layout_gravity="top"
                            android:textSize="14sp"
                            android:layout_marginLeft="-4dp"
                            android:textAlignment="center"/>

                        <charles.com.milu.CustomViews.TitleTextView
                            android:id="@+id/open_place_EventSec"
                            android:layout_width="44dp"
                            android:layout_height="42dp"
                            android:text = "47"
                            android:textColor="@color/Deepsky"
                            android:layout_gravity="center"
                            android:textSize="36sp"
                            android:textAlignment="center"/>

                        <charles.com.milu.CustomViews.TitleTextView
                            android:id="@+id/open_place_EventSecLabel"
                            android:layout_width="11dp"
                            android:layout_height="16dp"
                            android:text = "S"
                            android:layout_marginTop="6dp"
                            android:textColor="@color/color_Oldlavender"
                            android:layout_gravity="top"
                            android:textSize="14sp"
                            android:layout_marginLeft="-4dp"
                            android:textAlignment="center"/>

                    </LinearLayout>

                    <charles.com.milu.CustomViews.RegularTitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="san francisco museum of modern art"
                        android:id="@+id/umcomingHost_EvnetTitle"
                        android:layout_gravity="bottom"
                        android:textSize="20.0sp"
                        android:textColor="@color/white"
                        android:gravity="bottom"/>

                    <charles.com.milu.CustomViews.RegularTitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="museum"
                        android:id="@+id/place_type"
                        android:layout_gravity="bottom"
                        android:textSize="18.0sp"
                        android:textColor="@color/color_DimGray"
                        android:gravity="bottom"/>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:src="@drawable/divide_top_vd"/>

                </LinearLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:paddingRight="16dp"
                android:paddingLeft="16dp"
                android:layout_below="@+id/open_place_DetailLayout"
                android:id="@+id/open_place_GoingLayout">

                <charles.com.milu.CustomViews.TitleTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="OPEN NOW"
                    android:textSize="16sp"
                    android:layout_margin="@dimen/dimen_08"
                    android:textColor="@color/white"
                    android:layout_centerInParent="true"/>

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:src="@drawable/divide_bottom_vd"/>

            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/open_place_EventAddressLayout"
                android:layout_below="@+id/open_place_GoingLayout"
                android:layout_width="fill_parent"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:paddingTop="12dp"
                android:layout_height="wrap_content">


                <RelativeLayout
                    android:id="@+id/open_place_FriLayout"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/open_place_CalendarImage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/event_page_calendar_icon"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_toEndOf="@+id/open_place_CalendarImage"
                        >

                        <charles.com.milu.CustomViews.TitleTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/monday_friday_n9_00a_5_00p"
                            android:gravity="top"
                            android:layout_marginStart="12dp"
                            android:textColor="@color/colorWhite"
                            android:textSize="16sp"/>
                        <charles.com.milu.CustomViews.TitleTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/saturday_n11_00a_5_00p"
                            android:gravity="top"
                            android:layout_marginStart="12dp"
                            android:textColor="@color/colorWhite"
                            android:textSize="16sp"/>
                        <charles.com.milu.CustomViews.TitleTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/sunday_nclosed"
                            android:gravity="top"
                            android:layout_marginStart="12dp"
                            android:textColor="@color/colorWhite"
                            android:textSize="16sp"/>
                    </LinearLayout>


                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/event_page_lock_open"
                        android:layout_alignParentEnd="true"/>


                </RelativeLayout>


                <RelativeLayout
                    android:layout_below="@+id/open_place_FriLayout"
                    android:layout_width="fill_parent"
                    android:layout_height="38dp"
                    android:layout_marginTop="10dp"
                    android:layout_alignParentBottom="true">

                    <ImageView
                        android:id="@+id/open_place_DropPinImage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/event_page_drop_pin_icon"/>

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="203dp"
                        android:layout_height="wrap_content"
                        android:text="1372 Kreiger Plaza Suite 721San Francisco, CA"
                        android:gravity="top"
                        android:layout_toEndOf="@+id/open_place_DropPinImage"
                        android:layout_marginStart="12dp"
                        android:textColor="@color/colorWhite"
                        android:textSize="16sp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:background="@drawable/event_page_share_icon" />

                </RelativeLayout>


            </RelativeLayout>

            <RelativeLayout
                android:layout_marginTop="10dp"
                android:layout_width="fill_parent"
                android:id="@+id/open_place_MapLayout"
                android:layout_below="@+id/open_place_EventAddressLayout"
                android:background="@color/colorAccent"
                android:layout_height="120dp">

                    <com.google.android.gms.maps.MapView
                        android:id="@+id/open_place_Map"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:paddingRight="16dp"
                android:paddingLeft="16dp"
                android:paddingTop="7dp"
                android:id="@+id/open_place_ResumeLayout"
                android:layout_below="@+id/open_place_MapLayout">


                <RelativeLayout
                    android:id="@+id/open_place_HostedLayout"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="12dp"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/open_place_UserImage"
                        android:background="@drawable/event_page_user_icon"/>

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="created by john doe"
                        android:textColor="@color/colorWhite"
                        android:textSize="16sp"
                        android:layout_toEndOf="@+id/open_place_UserImage"
                        android:layout_marginStart="12dp"/>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/settings_next_icon3"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"/>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:paddingTop="7dp"
                    android:layout_below="@+id/open_place_HostedLayout">

                    <ImageView
                        android:id="@+id/open_place_DescriptIcon"
                        android:background="@drawable/event_page_description_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp" />

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_toEndOf="@+id/open_place_DescriptIcon"
                        android:id="@+id/open_place_DescriptionTxt"
                        android:layout_width="289dp"
                        android:layout_height="175dp"
                        android:textSize="16sp"
                        android:textColor="@color/colorWhite"
                        android:text="@string/upcomingHost_DescriptionTxt"
                        android:layout_marginStart="14dp"/>

                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/event_page_divide_line14"
                    android:layout_alignParentBottom="true"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="114dp"
                android:paddingRight="16dp"
                android:paddingLeft="16dp"
                android:paddingTop="8dp"
                android:layout_below="@+id/open_place_ResumeLayout"
                android:id="@+id/open_place_PeopleLayout">

                <charles.com.milu.CustomViews.RegularTitleTextView
                    android:id="@+id/open_place_PeoPleTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="114 people going"
                    android:textColor="@color/colorWhite"
                    android:textSize="16sp"/>

                <android.support.v7.widget.RecyclerView
                    android:layout_width="fill_parent"
                    android:layout_height="25dp"
                    android:orientation="vertical"
                    android:id="@+id/open_place_PeopleRecyclerView"
                    android:layout_below="@+id/open_place_PeoPleTxt"
                    android:layout_toLeftOf="@+id/next_button_1"
                    android:layout_marginTop="8dp">

                </android.support.v7.widget.RecyclerView>
                <charles.com.milu.utils.CustomImageButton
                    android:id="@+id/next_button_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/settings_next_icon"
                    android:layout_alignTop="@+id/open_place_PeopleRecyclerView"
                    android:layout_alignBottom="@+id/open_place_PeopleRecyclerView"
                    android:layout_alignParentEnd="true"/>

                <LinearLayout
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal"
                    android:layout_below="@+id/open_place_PeopleRecyclerView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/open_place_ShareIcon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/feed_share_icon"
                        android:layout_alignParentLeft="true"
                        />

                    <charles.com.milu.CustomViews.TitleTextView
                        android:id="@+id/open_place_InviteTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="INVITE PEOPLE"
                        android:textSize="16sp"
                        android:layout_marginLeft="16dp"
                        android:textColor="@color/Deepsky"
                        android:layout_gravity="center_vertical"
                        android:layout_alignParentRight="true"
                        android:textAlignment="center"/>

                </LinearLayout>


                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentBottom="true"
                    android:src="@drawable/event_page_divide_line14"/>


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="115dp"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:paddingTop="10dp"
                android:layout_below="@+id/open_place_PeopleLayout"
                android:id="@+id/open_place_LikeLayout">

                <charles.com.milu.CustomViews.RegularTitleTextView
                    android:id="@+id/open_place_LikeTitle"
                    android:text="256 people like this"
                    android:textColor="@color/colorWhite"
                    android:textSize="16sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    />

                <android.support.v7.widget.RecyclerView
                    android:layout_width="fill_parent"
                    android:layout_height="25dp"
                    android:id="@+id/open_place_LikeRecyclerView"
                    android:layout_below="@+id/open_place_LikeTitle"
                    android:layout_toLeftOf="@+id/next_button_2"
                    android:layout_marginTop="9dp">

                </android.support.v7.widget.RecyclerView>
                <charles.com.milu.utils.CustomImageButton
                    android:id="@+id/next_button_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/settings_next_icon"
                    android:layout_alignTop="@+id/open_place_LikeRecyclerView"
                    android:layout_alignBottom="@+id/open_place_LikeRecyclerView"
                    android:layout_alignParentEnd="true"/>


                <RelativeLayout
                    android:layout_width="172dp"
                    android:layout_height="24dp"
                    android:layout_below="@+id/open_place_LikeRecyclerView"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="16dp">

                    <ImageView
                        android:id="@+id/open_place_LikeIcon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/feed_like_icon"
                        android:layout_alignParentLeft="true"/>

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/open_place_LikeTxt"
                        android:text="LIKE THIS EVENT"
                        android:textSize="16sp"
                        android:textColor="@color/Deepsky"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"/>

                </RelativeLayout>

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:src="@drawable/event_page_divide_line14"/>


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/open_place_CommentLayout"
                android:paddingTop="8dp"
                android:paddingRight="16dp"
                android:paddingLeft="16dp"
                android:layout_below="@+id/open_place_LikeLayout">

                <charles.com.milu.CustomViews.RegularTitleTextView
                    android:layout_marginTop="10dp"
                    android:id="@+id/open_place_CommentsTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="26 commnets"
                    android:textColor="@color/colorWhite"
                    android:textSize="16sp"/>

                <android.support.v7.widget.RecyclerView
                    android:layout_width="wrap_content"
                    android:layout_below="@+id/open_place_CommentsTitle"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="9dp"
                    android:id="@+id/open_place_CommentRecyclerView">

                </android.support.v7.widget.RecyclerView>

                <LinearLayout
                    android:layout_marginTop="8dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="12dp"
                    android:layout_below="@+id/open_place_CommentRecyclerView">

                    <ImageView
                        android:id="@+id/open_place_CommentIcon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/feed_comment_icon"
                        android:layout_alignParentLeft="true"/>

                    <charles.com.milu.CustomViews.TitleTextView
                        android:id="@+id/open_place_CommentTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="COMMENT"
                        android:textColor="@color/Deepsky"
                        android:textSize="16sp"
                        android:layout_marginLeft="15dp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:layout_alignParentRight="true"
                        />

                </LinearLayout>

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:background="@color/color_DimGray"
                    android:layout_alignParentBottom="true"/>

                <charles.com.milu.utils.CustomImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/settings_next_icon"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"/>

            </RelativeLayout>

            <RelativeLayout
                android:layout_below="@+id/open_place_CommentLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:paddingTop="10dp"
                android:id="@+id/open_place_InterestsLayout">

                <charles.com.milu.CustomViews.RegularTitleTextView
                    android:id="@+id/open_place_InterestTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="interests"
                    android:textSize="16sp"
                    android:textColor="@color/colorWhite"/>

                <LinearLayout
                    android:layout_below="@+id/open_place_InterestTitle"
                    android:id="@+id/open_place_In_Gra_Layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:orientation="horizontal">

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/open_place_InternetTxt"
                        android:text="internet of things"
                        android:textColor="@color/colorWhite"
                        android:background="@drawable/button_bordered_white"
                        android:textSize="17sp"/>

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/open_place_GraphicsTxt"
                        android:text="internet of things"
                        android:textColor="@color/color_Milumain"
                        android:layout_marginLeft="12dp"
                        android:background="@drawable/button_bordered_blue"
                        android:textSize="17sp"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_below="@+id/open_place_In_Gra_Layout"
                    android:id="@+id/open_place_New_Cry_Layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:orientation="horizontal">

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/open_place_NewTxt"
                        android:text="new technology"
                        android:textColor="@color/colorWhite"
                        android:background="@drawable/button_bordered_white"
                        android:textSize="17sp"/>

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/open_place_CryTxt"
                        android:text="cryptocurrency"
                        android:textColor="@color/colorWhite"
                        android:layout_marginLeft="12dp"
                        android:background="@drawable/button_bordered_white"
                        android:textSize="17sp"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_below="@+id/open_place_New_Cry_Layout"
                    android:id="@+id/open_place_Com_Uni_Layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="16dp"
                    android:orientation="horizontal">

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/open_place_ComTxt"
                        android:text="comoputer programming"
                        android:textColor="@color/color_Milumain"
                        android:background="@drawable/button_bordered_blue"
                        android:textSize="17sp"/>

                    <charles.com.milu.CustomViews.TitleTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/open_place_UniTxt"
                        android:text="unity"
                        android:textColor="@color/colorWhite"
                        android:layout_marginLeft="12dp"
                        android:background="@drawable/button_bordered_white"
                        android:textSize="17sp"/>

                </LinearLayout>

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/event_page_divide_line14"
                    android:layout_below="@+id/open_place_Com_Uni_Layout"
                    />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/open_place_past_event"
                android:layout_below="@+id/open_place_InterestsLayout"
                android:layout_width="match_parent"
                android:paddingRight="16dp"
                android:paddingLeft="16dp"
